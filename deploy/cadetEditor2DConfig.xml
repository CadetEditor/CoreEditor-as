<xml>
	<applicationID><![CDATA[cadetEditor2D]]></applicationID>
	
	<fileSystemProvider environment="air"		    type="local" 		    id="cadet.local" 		    	label="Cadet2D"     folderName="Cadet2D" />
	<fileSystemProvider environment="browser" 	type="sharedObject" id="cadet.sharedObject" 	label="Cadet2D" />
	<fileSystemProvider environment="browser" 	type="url" 			    id="cadet.url" 			      label="Cadet2D_URL" baseURL="files" visible="false" />
	
	<extension url="extensions/DefaultExtension.swf" />
	<extension url="extensions/CadetEditorExtension.swf" />
	<extension url="extensions/CadetEditorExtension2D.swf" />
	
	<manifest>
		<url><![CDATA[extensions/manifests/Flox.xml]]></url>
	</manifest>

	<manifest>
		<url><![CDATA[extensions/manifests/Cadet.xml]]></url>
	</manifest>

	<manifest>
		<url><![CDATA[extensions/manifests/Cadet2D.xml]]></url>
	</manifest>

	<manifest>
		<url><![CDATA[extensions/manifests/Cadet2DFlash.xml]]></url>
	</manifest>
	
  <manifest>
		<url><![CDATA[extensions/manifests/Cadet2DStarling.xml]]></url>
	</manifest>

	<manifest>
		<url><![CDATA[extensions/manifests/Cadet2DBox2D.xml]]></url>
	</manifest>

	<manifest>
		<url><![CDATA[extensions/manifests/CadetEditor.xml]]></url>
	</manifest>
	
	<!--  File templates  -->

	<template>
		<label><![CDATA[Barebones 2D with Physics]]></label>
		<description><![CDATA[Creates a Cadet Editor scene with a Renderer2D and a Physics process.]]></description>
		<url><![CDATA[extensions/fileTemplates/barebones2DPhysics.cdt2d]]></url>
		<icon>cadetEditor.assets.CadetEditorIcons_BlankTemplateIcon</icon>
	</template>

	<template>
		<label><![CDATA[Motorbike Template]]></label>
		<description><![CDATA[A simple motorbike physics example with editable terrain.]]></description>
		<url><![CDATA[extensions/fileTemplates/motorbike_bridge.cdt2d]]></url>
		<icon>cadetEditor.assets.CadetEditorIcons_BlankTemplateIcon</icon>
	</template>
	
	<template>
		<label><![CDATA[Marble Run Template]]></label>
		<description><![CDATA[A simple marble run example.]]></description>
		<url><![CDATA[extensions/fileTemplates/marble_run.cdt2d]]></url>
		<icon>cadetEditor.assets.CadetEditorIcons_BlankTemplateIcon</icon>
	</template>
	
	<template>
		<label><![CDATA[Race Track Template]]></label>
		<description><![CDATA[A plan view marble run example.]]></description>
		<url><![CDATA[extensions/fileTemplates/racer.cdt2d]]></url>
		<icon>cadetEditor.assets.CadetEditorIcons_BlankTemplateIcon</icon>
	</template>		

</xml>